{
    "doctype": "Etsy Sync Log",
    "name": "Etsy Sync Log",
    "module": "Etsy ERPNext",
    "custom": 0,
    "fields": [
        {
            "fieldname": "sync_type",
            "fieldtype": "Select",
            "label": "Sync Type",
            "options": "\nOrder Sync\nAuthorization Sync\nOther",
            "reqd": 1,
            "description": "Type of synchronization operation."
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "\nSuccess\nFailure",
            "reqd": 1,
            "description": "Indicates whether the sync operation was successful or failed."
        },
        {
            "fieldname": "start_time",
            "fieldtype": "Datetime",
            "label": "Start Time",
            "reqd": 1,
            "description": "The start time of the synchronization operation."
        },
        {
            "fieldname": "end_time",
            "fieldtype": "Datetime",
            "label": "End Time",
            "description": "The end time of the synchronization operation."
        },
        {
            "fieldname": "etsy_order_id",
            "fieldtype": "Data",
            "label": "Etsy Order ID",
            "description": "The ID of the Etsy order if applicable."
        },
        {
            "fieldname": "message",
            "fieldtype": "Text",
            "label": "Message",
            "description": "Detailed message or error information for the synchronization."
        },
        {
            "fieldname": "log_type",
            "fieldtype": "Select",
            "label": "Log Type",
            "options": "\nInfo\nWarning\nError",
            "default": "Info",
            "description": "Type of log message."
        },
        {
            "fieldname": "created_by",
            "fieldtype": "Data",
            "label": "Created By",
            "read_only": 1,
            "description": "The user or system that created this log entry."
        }
    ],
    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        }
    ]
}
